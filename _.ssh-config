ServerAliveCountMax = 5
ServerAliveInterval = 30

ControlMaster auto
ControlPath /tmp/ssh_mux_%h_%p_%r

ForwardAgent yes
#ControlPersist 1h

host bitbucket.com
  user git
  hostname bitbucket.com
  port 22
  identityfile ~/.ssh/bitbucket

host github.com
  user git
  hostname github.com
  port 22
  identityfile ~/.ssh/github

host gitcafe.com
  user git
  hostname gitcafe.com
  port 22
  identityfile ~/.ssh/gitcafe

# vim: ft=sshconfig

